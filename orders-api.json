  /v1/chronopost/customer:
    post:
      tags:
      - chronopost
      summary: Create Chronopost customer
      description: |
        Registers a new recipient for Chronopost shipping.
        Use this endpoint to provide all necessary address and contact details for the recipient.
        The response contains the created customer information or an error if the request is invalid.
      requestBody:
        description: Customer details required for Chronopost shipment.
        content:
          application/json:
            schema:
              type: object
              properties:
                recipient_phone:
                  type: string
                  description: Recipient's phone number (E.164 format recommended).
                recipient_address1:
                  type: string
                  description: Primary address line of the recipient.
                recipient_city:
                  type: string
                  description: City of the recipient.
                recipient_country_code:
                  type: string
                  description: ISO country code (e.g., 'FR').
                recipient_country:
                  type: string
                  description: Full country name (e.g., 'France').
                recipient_name:
                  type: string
                  description: Full name of the recipient.
                recipient_name2:
                  type: string
                  description: Additional name information (optional).
                recipient_civility:
                  type: string
                  description: Civility or title (e.g., 'Mr', 'Ms').
                recipient_email:
                  type: string
                  description: Email address of the recipient.
                recipient_zip:
                  type: string
                  description: Postal code of the recipient.
                store:
                  type: string
                  description: Store ID (24-character hex string).
                type:
                  type: string
                  description: Label format. Allowed values: PDF, ZPL, BUFF.
      responses:
        "200":
          description: Customer created successfully.
          content:
            application/json:
              schema:
                type: object
        "400":
          description: Invalid request. Check the provided data.
          content:
            application/json:
              schema:
                type: object
                properties:
                  tsl:
                    type: string
                    default: invalid_request
        "401":
          description: Invalid or missing authentication token.
          content:
            application/json:
              schema:
                type: object
                properties:
                  tsl:
                    type: string
                    default: invalid_token
        "403":
          description: Access denied. You do not have permission to perform this action.
          content:
            application/json:
              schema:
                type: object
                properties:
                  tsl:
                    type: string
                    default: access_denied
      x-codegen-request-body-name: body
  /v1/chronopost/lpt:
    post:
      tags:
      - chronopost
      summary: Create Chronopost shipment between stores
      description: |
        Initiates a shipment between two stores using Chronopost.
        Provide sender and recipient store IDs, package dimensions, and recipient details.
        The response includes shipment information or an error if the request is invalid.
      requestBody:
        description: Shipment details including sender, recipient, and package information.
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: string
                  description: Sender store ID (24-character hex string).
                to:
                  type: string
                  description: Recipient store ID (24-character hex string).
                height:
                  type: number
                  description: Package height in centimeters.
                width:
                  type: number
                  description: Package width in centimeters.
                length:
                  type: number
                  description: Package length in centimeters.
                weight:
                  type: number
                  description: Package weight in kilograms.
                recipient_name2:
                  type: string
                  description: Additional recipient name information (optional).
                recipient_civility:
                  type: string
                  description: Civility or title (e.g., 'Mr', 'Ms').
                recipient_email:
                  type: string
                  description: Email address of the recipient.
                recipient_zip:
                  type: string
                  description: Postal code of the recipient.
                type:
                  type: string
                  description: Label format. Allowed values: PDF, ZPL, BUFF.
      responses:
        "200":
          description: Shipment created successfully.
          content:
            application/json:
              schema:
                type: object
        "400":
          description: Invalid request. Check the provided data.
          content:
            application/json:
              schema:
                type: object
                properties:
                  tsl:
                    type: string
                    default: invalid_request
        "401":
          description: Invalid or missing authentication token.
          content:
            application/json:
              schema:
                type: object
                properties:
                  tsl:
                    type: string
                    default: invalid_token
        "403":
          description: Access denied. You do not have permission to perform this action.
          content:
            application/json:
              schema:
                type: object
                properties:
                  tsl:
                    type: string
                    default: access_denied
